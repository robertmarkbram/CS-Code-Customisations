{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Link_to_Facebook": {
		"prefix": "linkFacebook",
		"body": [
			"<a target=\"_blank\" data-toggle=\"tooltip\" title=\"Link to ${1:dogName}&#8217;s Facebook post opens in a new tab.\" href=\"${TM_SELECTED_TEXT}\">${2:Facebook post for ${1:dogName}}</a>"
		],
		"description": "Link to Blog Roll"
	},
	"Link_to_Blog": {
		"prefix": "linkBlog",
		"body": [
			"<a target=\"_blank\" data-toggle=\"tooltip\" title=\"Link to ${1:dogName}&#8217;s Chihuahua Rescue Victoria Dog Roll entry opens in a new tab.\" href=\"${TM_SELECTED_TEXT}\">${2:Dog Roll entry for ${1:dogName}}</a>"
		],
		"description": "Link to Blog Roll"
	},
	"html_p": {
		"prefix": "p",
		"body": [
			"<p>${TM_SELECTED_TEXT}</p>"
		],
		"description": "HTML paragraph"
	},
	"html_p_date": {
		"prefix": "pdate",
		"body": [
			"<p class=\"date\">${TM_SELECTED_TEXT}</p>"
		],
		"description": "HTML paragraph with date class"
	}
}